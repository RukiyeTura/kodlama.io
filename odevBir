#3
#login
kayitliMail = "example@abc.com"
kayitliSifre = "1234"
mail = input('mail: ')
sifre = input('sifre: ')

if (mail == kayitliMail and sifre == kayitliSifre):
        print("Giris basarili")
else:
        print("Mail ya da sifre yanlis")